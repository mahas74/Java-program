import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner o = new Scanner(System.in);
        int n = o.nextInt();
        int first = 1, second = 1;
        for (int i = 1; i <= n; i++) {
            System.out.print(first + " ");
            int next = first + second;
            first = second;
            second = next;
        }
    }
}

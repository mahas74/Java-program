import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner o = new Scanner(System.in);
        int n = o.nextInt();
        int start = 1;
        int incre = 1;
        for (int i = 0; i < n; i++) {
            System.out.print(start + " ");
            incre++;
            start += incre;
        }
    }
}
